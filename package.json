    "@types/node-fetch": "^2.5.4",
    "chai": "^4.2.0",
    "factom-dev-stack": "^0.2.4",
    "mocha": "^7.0.1",
    "rimraf": "^3.0.2",
    "typescript": "^3.8.2"
  }
